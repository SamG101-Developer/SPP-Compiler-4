mod testing::pinning


cls PinTest {
    a: std::BigInt
    b: std::BigInt
}


fun test_pinning_async(string: &PinTest) -> std::Void { }

cor test_pinning_coro(string: &PinTest) -> std::GenRef[std::BigInt] { }


fun test_pinning() -> PinTest {
    let pin_test = PinTest(a=1, b=2)
    pin pin_test

    let future = async test_pinning_async(&pin_test)

    rel pin_test
    ret pin_test
}
